            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"dimensions\": [\"facilityId\", \"date\"],\n  \"measures\": [\"patientCount\", \"avgLOS\"],\n  \"filters\": {},\n  \"timePeriod\": {\n    \"from\": \"2024-01-01\",\n    \"to\": \"2024-01-31\"\n  },\n  \"limit\": 1000\n}"
            },
            "url": {
